        <div class="active tab-pane" id="atendimento">
            <div class="row">
                <div class="col-xs-12 col-sm-6 col-md-6 top-bar">
                    <p class="titulo-formulario">Lista de Atendimento</p>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-6 top-bar">
                    @* <a title="Clique aqui para adicionar um novo tipo de atendimento" class="btn btn-primary pull-right" href="@Url.Action("Adicionar", "TipoAtendimento", new {Area = "Administrativo"})"><i class="fa fa-plus-circle"></i> Adicionar</a> *@
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <table class="table table-hover" width="100%" data-table="true">
                        <thead>
                            <tr>
                                <th>Código</th>
                                <th>Descrição</th>
                                <th>Ações</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var obj in Model.TipoAtendimento)
                            {
                                <tr>
                                    <td>@obj.TpAt_ID_TipoAtendimento</td>
                                    <td>@obj.TpAt_NM_Descricao</td>
                                    <td>
                                        <a class="btn btn-info" href="@Url.Action("Editar", "TipoAtendimento", new {Area = "Administrativo", codTipoAtendimento = obj.TpAt_ID_TipoAtendimento})" title="Editar"><i class="fa fa-edit"></i></a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                        <thead>
                            <tr>
                                <td colspan="3">&nbsp;</td>
                            </tr>
                        </thead>
                        <tfoot></tfoot>
                    </table>
                </div>
            </div>
        </div>
