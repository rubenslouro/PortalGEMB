<div class="nav-tabs-custom">
    <ul class="nav nav-tabs">
        <li class="active"><a href="#atendimento" data-toggle="tab">Atendimento</a></li>
        <li><a href="#atividaderemunerada" data-toggle="tab">Atividade Remunerada</a></li>
        <li><a href="#dependente" data-toggle="tab">Dependente</a></li>
