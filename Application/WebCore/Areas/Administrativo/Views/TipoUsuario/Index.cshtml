@model Domain.Dtos.TipoUsuario.Listar.Output.TipoUsuarioListarOutModel
@{
    ViewData["Title"] = "Lista de Tipo de Usuário";
    Layout = "~/Areas/Administrativo/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12 top-bar">
        <a title="Clique aqui para adicionar um novo tipo de usuário" class="btn btn-primary pull-right" href="@Url.Action("Adicionar", "TipoUsuario", new {Area = "Administrativo"})"><i class="fa fa-plus-circle"></i> Tipo Usuário</a>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <table class="table  table-hover" width="100%" data-table="true">
            <thead>
                <tr>
                    <th>Código</th>
                    <th>Descrição</th>
                    <th>Ações</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model.ListaTiposUsuario)
                {
                    <tr>
                        <td class="text-right">@obj.Codigo</td>
                        <td>@obj.Descricao</td>
                        <td>
                            <a class="btn btn-info" href="@Url.Action("Configuracoes", "TipoUsuario", new {Area = "Administrativo", codTipoUsuario = obj.Codigo})" title="Configurações de tipo usuário"><i class="fa fa-gear"></i></a>
                            <a class="btn btn-info" href="@Url.Action("Editar", "TipoUsuario", new {Area = "Administrativo", codTipoUsuario = obj.Codigo})" title="Editar este usuário"><i class="fa fa-edit"></i></a>
                        </td>
                    </tr>
                }
            </tbody>
            <tfoot></tfoot>
        </table>
    </div>
</div>

