@{
    ViewData["SiteName"] = "Portal GEMB";
}
<!DOCTYPE html>
<html lang="pt-BR">
@await Html.PartialAsync("~/Areas/Administrativo/Views/Partial/LayoutHead.cshtml")
<body class="container image-forced-background-extra">
    
    <div class="row" id="wrapper">
        <div class="col-lg-12" style="margin-top: 30px;">           
            @RenderBody()
        </div>
    </div>
    @await Html.PartialAsync("~/Areas/Administrativo/Views/Partial/Dialogs.cshtml")
    @await Html.PartialAsync("~/Areas/Administrativo/Views/Partial/ScriptsGeral.cshtml")

    @await RenderSectionAsync("scripts", required: false)
</body>
</html>
